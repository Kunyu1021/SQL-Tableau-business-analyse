s/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">CREATE TABLE ai_market_summary AS
SELECT
    country,
    industry,
    ai_tool,
    year,
    age_group,
    company_size,
    AVG(adoption_rate)       AS avg_adoption_rate,
    SUM(daily_active_users)  AS total_dau,
    COUNT(*)                 AS records_count
FROM ai_raw
GROUP BY
    country,
    industry,
    ai_tool,
    year,
    age_group,
    company_size;

SELECT * FROM ai_market_summary;
</sql><current_tab id="0"/></tab_sql></sqlb_project>
